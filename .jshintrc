// http://www.jshint.com/docs/options/
{
  "node": true,
  "browser": true,
  "jquery": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "singledouble",
  "regexp": true,
  "undef": true,
  "unused": false,
  "strict": false,
  "trailing": true,
  "smarttabs": true,
  "globals": {
    "angular": false,
    "intbarOptions": true,
    "Datamap": true,
    "intbarMapColors": true,
    "intbarMapGoal": true,
    "d3": true
  }
}
